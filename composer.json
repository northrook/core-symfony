{
  "name": "northrook/core-symfony",
  "description": "Core Symfony integration.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Martin",
      "email": "mn@northrook.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Core\\Symfony\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.3",
    "symfony/http-kernel": "^7.1",
    "northrook/filesystem": "dev-main",
    "northrook/support": "dev-main",
    "symfony/dependency-injection": "^7.1",
    "symfony/yaml": "^7.1",
    "northrook/logger": "dev-main",
    "northrook/clerk": "dev-main"
  },
  "require-dev": {
    "symfony/framework-bundle": "^7.2",
    "northrook/dev-env": "dev-main"
  },
  "scripts": {
    "phpstan": "vendor/bin/phpstan analyse"
  }
}
