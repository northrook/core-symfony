{
  "name": "northrook/core-symfony",
  "description": "Core Symfony integration.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Martin",
      "email": "mn@northrook.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Core\\Symfony\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.3",
    "symfony/dependency-injection": "@stable",
    "symfony/http-foundation": "@stable",
    "symfony/yaml": "@stable",
    "symfony/cache": "@stable",
    "northrook/core": "dev-main",
    "northrook/logger": "dev-main",
    "northrook/pathfinder": "dev-main",
    "northrook/support": "dev-main",
    "northrook/php-class-finder": "dev-main"
  },
  "require-dev": {
    "northrook/dev": "dev-main"
  },
  "scripts": {
    "phpstan": "vendor/bin/phpstan analyse src"
  }
}
